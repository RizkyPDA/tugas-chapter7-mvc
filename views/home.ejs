								<!-- Link Detail Untuk Tampilan Laptop -->
								<a href="/detail/<%= element.uuid %>">DETAIL <i class="fas fa-angle-double-right"></i></a>
							</div>
						</div>
						<div class="portfolio__card-body">
							<h3><%= element.project_name %></h3>
							<p class="text-muted"><%= element.category %></p>
							<p class="text-muted"><%= new Date(element.release_date).toLocaleDateString('id-ID') %></p>
